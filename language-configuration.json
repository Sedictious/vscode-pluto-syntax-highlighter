{
    "comments": {
        // symbols used for block comments
        "blockComment": [ "/*", "*/" ]
    },
    // symbols used as brackets
    "brackets": [
        ["[", "]"],
        ["(", ")"]
    ],
    // symbols that are auto closed
    "autoClosingPairs": [
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""]
    ],
    // symbols that that can surround a selection
    "surroundingPairs": [
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ]
}